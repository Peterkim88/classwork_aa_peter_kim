<p id="notice"><%= notice %></p>

<h1>Full Sets</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @full_sets.each do |full_set| %>
      <tr>
        <td><%= link_to 'Show', full_set %></td>
        <td><%= link_to 'Edit', edit_full_set_path(full_set) %></td>
        <td><%= link_to 'Destroy', full_set, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Full Set', new_full_set_path %>
